{
	"name": "artworked",
	"homepage": "https://edwindelbosque.github.io/artworked/",
	"version": "0.1.0",
	"private": true,
	"proxy": "http://localhost:3001",
	"dependencies": {
		"node-sass": "^4.13.0",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "5.0.1",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
